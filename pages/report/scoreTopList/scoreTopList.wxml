<view class='bg bg{{currentTab}}'>
  <view class='bg-h1'>学习排行榜</view>
  <text wx:if="{{currentTab == 0}}" class='bg-p'>学贵得师,亦贵得友。</text>
  <text wx:elif="{{currentTab == 1}}" class='bg-p'>读万卷书,下笔如有神。</text>
</view>
<view class="swiper-tab">
