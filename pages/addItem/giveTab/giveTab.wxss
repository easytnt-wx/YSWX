page{
  display: flex;
  flex-direction: column;
  height: 100%;
}
swiper{
  flex: auto;
  overflow: auto;
}
swiper-item{
  overflow: auto;
}
.swiper-tab{
    width: 100%;
    border-bottom: 1rpx solid #e5e5e5;
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
    display: flex;
    flex-flow: row;
    justify-content: space-between;
    flex: none;
}
.swiper-tab-item{
    width: 30%; 
    color:#c9d1d0;
    font-size: 18px;
}
.active{
    color:#3e4b4b;
    border-bottom: 4rpx solid #01e6ad;
}
.grades{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin-top: 25rpx;
  margin-left: 40rpx;
}
.grade-item{
  width: 50%;
  box-sizing: border-box;
  padding: 0 40rpx 40rpx 0;
}
.item-box{
  border-radius: 20rpx;
  box-shadow: 2rpx 4rpx 40rpx 4rpx #efefef;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.item-active{
   box-shadow: 2rpx 4rpx 20rpx 4rpx #ddd;
}
.item-logo{
  width: 130rpx;
  height: 130rpx;
  margin-top: 40rpx;
  background-size: cover;
  display: flex;
  justify-content: flex-end;
}
