.container{
  padding: 70rpx 60rpx 0 60rpx;
}
.h1{
  font-size: 24px;
  line-height: 50rpx;
  color: #3e4b4b;
}
.h1 > text{
  font-size: 14px;
  color: #c9d1d0;
  line-height: 40rpx;
}
.person-box{
  margin-top: 48rpx;
}
.person-box .h2{
  color: #fff;
  font-size: 24px;
  line-height: 50rpx;
  margin: 25rpx 0 8rpx 0;
}
.person-box .h2+text{
  color: #fff;
  font-family: Arial, Helvetica, sans-serif;
  font-size: 16px;
}
.icon-teacher{
  height: 230rpx;
  background-image: url(http://192.168.1.252/public/static/easy_exam/YSWX/images/teacher.png);
  background-size: cover;
  background-color: #fff;
  border-radius: 8px;
  margin-bottom: 40rpx;
  padding: 55rpx 50rpx;
  box-sizing: border-box;
}
